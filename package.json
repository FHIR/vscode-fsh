{
    "name": "vscode-language-fsh",
    "displayName": "vscode-language-fsh",
    "description": "VSCode FHIR Shorthand (FSH) Language Support",
    "version": "1.0.6",
    "author": "The MITRE Corporation",
    "license": "Apache-2.0",
    "publisher": "kmahalingam",
    "repository": {
        "type": "git",
        "url": "https://github.com/standardhealth/vscode-language-fsh"
    },
    "engines": {
        "vscode": "^1.39.0"
    },
    "activationEvents": [
        "onLanguage:fsh"
    ],
    "main": "./out/extension",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fsh",
                "aliases": [
                    "FSH",
                    "fsh"
                ],
                "extensions": [
                    ".fsh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fsh",
                "scopeName": "source.fsh",
                "path": "./syntaxes/fsh.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "fsh",
                "path": "./snippets/fsh-snippets.json"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/antlr4": "^4.7.2",
        "@types/node": "^14.14.25",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "antlr4": "~4.8.0"
    }
}
